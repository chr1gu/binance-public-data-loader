  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'binance-get'",
      "cargo": {
        "args": ["build", "--bin=binance-get", "--package=binance-get"],
        "filter": {
          "name": "binance-get",
          "kind": "bin"
        }
      },
      "args": [
        "fetch",
        "--symbol=USDT$",
        "--interval=1m",
        "--start-date=2021-03-01",
        "--end-date=2022-08-01",
        "./target"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'binance-get'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=binance-get",
          "--package=binance-get"
        ],
        "filter": {
          "name": "binance-get",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
